<%- include('partials/header') %>
<body>
<div class="jumbotron animate__animated animate__fadeInDown" id="login">
  <div class=" d-flex justify-content-center animate__animated animate__flip ">
    <img src="data:image/png;base64,"/>
  </div>
  <h3 class="text-center animate__animated animate__flip ">Register</h3>
  <div class="d-flex justify-content-center form animate__animated animate__lightSpeedInLeft">
      <form action="/signup" method="POST">
          <div class="form-group">
            <label for="exampleInputEmail1">Email address</label>
            <div class="input-group mb-2">
              <div class="input-group-prepend">
                <div class="input-group-text">
                    <img src="data:image/png;base64,"/>
                </div>
              </div>
              <input type="email" class="form-control  text-center" name="email" id="exampleInputEmail1"
              aria-describedby="emailHelp" placeholder="Enter email" required>
            </div>   
          </div>
          <div class="form-group">
            <label for="exampleInputEmail1">Password</label>
            <div class="input-group mb-2">
              <div class="input-group-prepend">
                <div class="input-group-text">
                    <img src="data:image/png;base64,"/>
                </div>
              </div>
              <input type="password" name="password" class="form-control  text-center" id="exampleInputPassword1"
                placeholder="Password" required>
            </div>
          </div>   
          <div class="form-group">
            <label for="exampleInputEmail1">Confirm Password</label>
            <div class="input-group mb-2">
              <div class="input-group-prepend">
                <div class="input-group-text">
                    <img src="data:image/png;base64,"/>
                </div>
              </div>
              <input type="password" name="cpass" class="form-control  text-center" id="exampleInputPassword2"
            placeholder="Password" required>
        </div> 
      </div> 
        <button type="submit" class="btn btn-md btn-block mb-3">Submit</button>
          <a href="/login">login</a>
          <div class="form-inline">
            <label for="exampleInputEmail1"></label>
            <div class="input-group mb-2">
              <div class="input-group-prepend" id="img">
                <div class="input-group-text">
                  <img src="../images/google.png" width="20px" height="20px">
                </div>
              </div>  
                <a href="/auth/google" id="google" type="text" class="text-dark form-control" >Signup with Google</a>
            </div>
        </div>
      </form>
  </div>
</div>
</body>

</html>

<%- include('partials/footer') %>